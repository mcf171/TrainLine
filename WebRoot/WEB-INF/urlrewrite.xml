<?xml version="1.0" encoding="UTF-8"?>
<urlrewrite>
<rule>
	<from>/index.html</from>
	<to type="forward">${basePath}/login.jsp</to>
</rule>
<!-- 将新闻内容映射为/news-contentId-moduleId.html -->
<rule>
	<from>^/news-([0-9]+)-([0-9]+).html$</from>
	<to type="forward">
		/queryModuleContent.action?contentId=$1&amp;moduleId=$2
	</to>
</rule> 
<!-- 将问卷映射为/questionnaire/questionnaireArrangementId.html -->
<rule>
	<from>^/questionnaireArrangement/([0-9]+).action$</from>
	<to type="forward">
	/getQuestionnaireArrangePage.action?questionnaireArrangement.questionnaireArrangementId=$1
	</to>
</rule>

<rule>
	<from>^/getFile/(upload/\w+/([0-9]|\w)+\..+)$</from>
	<to type="forward">
	/getDownloadFile.action?fileName=$1
	</to>
</rule>
</urlrewrite> 