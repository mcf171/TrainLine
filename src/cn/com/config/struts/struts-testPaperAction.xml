<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="testPaper" namespace="/admin" extends="system">
		<action name="testPaper_*" class="testPaperAction" method="{1}">
		</action>
		
		<action name="getAddTestPaperPage" class="testPaperAction" method="getAddTestPaperPage">
			<result>/WEB-INF/jsp/backend/test/addTest.jsp</result>
		</action>
		<action name="addTestPaper" class="testPaperAction" method="addTestPaper">
			<result>/WEB-INF/jsp/backend/test/questionareOpen.jsp</result>
		</action>
		<action name="modifyTestPaper" class="testPaperAction" method="modifyTestPaper">
			<result>/WEB-INF/jsp/backend/test/questionareOpen.jsp</result>
		</action>
		<action name="deleteTestPaper" class="testPaperAction" method="deleteTestPaper">
			<result>/WEB-INF/jsp/backend/test/questionareOpen.jsp</result>
		</action>
		<action name="getExaminations" class="testPaperAction" method="getExaminations">
			<result>/WEB-INF/jsp/backend/examinations.jsp</result>
		</action>
		
		<action name="getOpenTestPaperPage" class="testPaperAction" method="getOpenTestPaperPage">
			<result>/WEB-INF/jsp/backend/test/questionareOpen.jsp</result>
		</action>
		
		<action name="getKeepTestPaperPage" class="testPaperAction" method="getKeepTestPaperPage">
			<result>/WEB-INF/jsp/backend/test/questionareKeep.jsp</result>
		</action>
		
		<action name="getRubbleTestPaperPage" class="testPaperAction" method="getRubbleTestPaperPage">
			<result>/WEB-INF/jsp/backend/test/questionareRubble.jsp</result>
		</action>
		
		<action name="findOpenTestPaper" class="testPaperAction" method="findOpenTestPaper">
			<result type="json">
				<param name="root">dataMap</param>
				</result>
		</action>
		
		<action name="findRubbleTestPaper" class="testPaperAction" method="findRubbleTestPaper">
			<result type="json">
				<param name="root">dataMap</param>
				</result>
		</action>
		
		<action name="findKeepTestPaper" class="testPaperAction" method="findKeepTestPaper">
			<result type="json">
				<param name="root">dataMap</param>
				</result>
		</action>
		
		
		
	</package>
</struts>    
