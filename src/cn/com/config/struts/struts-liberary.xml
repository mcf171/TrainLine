<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	
	<package name="liberary" extends="system" >
		<action name="getInsideLiberaryList" class="liberaryAction" method="getInsideLiberaryList">
				<result type="json">
				<param name="root">dataMap</param>
				</result>
	   	</action>
	   	
	   	<action name="getOutSideLiberaryList" class="liberaryAction" method="getOutSideLiberaryList">
				<result type="json">
				<param name="root">dataMap</param>
				</result>
	   	</action>
	   	
	   	<action name="getDangkeLiberaryList" class="liberaryAction" method="getDangkeLiberaryList">
				<result type="json">
				<param name="root">dataMap</param>
				</result>
	   	</action>
   	
   		<action name="readBook" class="liberaryAction" method="readBook">
   			<result>/WEB-INF/jsp/read.jsp</result>
   		</action>
   		
   		<action name="showNormalLiberaryListPage" class="liberaryAction" method="showNormalLiberaryListPage">
   			<result>/WEB-INF/jsp/liberary.jsp</result>
   		</action>
   		
   		
   		<action name="showBackendLiberaryListPage" class="liberaryAction" method="showBackendLiberaryListPage">

   			<result>/WEB-INF/jsp/backend/liberary.jsp</result>

   		</action>
   		
   		<action name="showBackendInsideLiberaryListPage" class="liberaryAction" method="showBackendInsideLiberaryListPage">

   			<result>/WEB-INF/jsp/backend/liberary/insideLiberary.jsp</result>

   		</action>
   		
   		<action name="showBackendOutSideLiberaryListPage" class="liberaryAction" method="showBackendInsideLiberaryListPage">

   			<result>/WEB-INF/jsp/backend/liberary/outsideLiberary.jsp</result>

   		</action>
   		
   		<action name="showBackendDangkeLiberaryListPage" class="liberaryAction" method="showBackendInsideLiberaryListPage">

   			<result>/WEB-INF/jsp/backend/liberary/dangkeLiberary.jsp</result>

   		</action>
   		
   	</package>
</struts>    
