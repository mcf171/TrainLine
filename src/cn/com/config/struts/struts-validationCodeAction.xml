<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<package name="util" extends="system">


		<action name="validationCode" class="cn.com.action.ValidationCodeAction">
			<result name="success" type="stream">
				<param name="contentType">image/jpeg</param>
				<param name="inputName">imageStream</param>
				<param name="bufferSize">2048</param>
			</result>

		</action>
		<action name="validCode" class="cn.com.action.ValidationCodeAction" method="validCode">
		    <result type="json">
			<param name="root">dataMap</param>
			</result>
   		 </action>
		<action name="notFound">
			<result>/WEB-INF/jsp/common/Error.jsp</result>
		</action>
	</package>
	
	
</struts>    
