<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="questionnairerubric" namespace="/" extends="json-default">
		<action name="questionnairerubric_*" class="questionnairerubRicAction" method="{1}">
		<result name="success">/backend/questionnaire/manage-show.html</result>
		</action>
	</package>
	
	<package name="questionnaireRubricAdmin" namespace="/admin" extends="system">
		<action name="addQuestionnaireRubric" class="questionnaireRubricAction" method="addQuestionnaireRubric">
			<result type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		
		<action name="deleteQuestionnaireRubric" class="questionnaireRubricAction" method="deleteQuestionnaireRubric">
			<result type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
	</package>
	
	<package name="adminQuestionnaireRubric" namespace="/admin/questionnaireRubric" extends="system">
		<action name="batchUpload" class="questionnaireRubricAction" method="batchUpload">
			<result>/utill.jsp</result>
		</action>
	</package>
</struts>    
