<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="note" namespace="/" extends="system">
		<action name="note_*" class="noteAction" method="{1}">
			<result type="json" name="success">
			<param name="root">dataMap</param>
			</result>
			<result name="showGonggao">/WEB-INF/jsp/backend/study.jsp</result>
			<result name="showNotePage">/WEB-INF/jsp/backend/study/note-overview.jsp</result>
		</action>
	</package>
	
	<package name="noteAdmin" namespace="/admin" extends="system">
		<action name="getNoticeList" class="noteAction" method="getNoticeList">
			<result>/WEB-INF/jsp/backend/study.jsp</result>
		</action>
	</package>
	
</struts>    
