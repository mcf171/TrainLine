<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="credential" namespace="/" extends="json-default">
		<action name="credential_*" class="credentialAction" method="{1}">
		<result type="json" name="success">
		<param name="root">dataMap</param>
		</result>
		<result name="intoCertificatePage">/WEB-INF/jsp/backend/classes/certificateList.jsp</result>
		</action>
	</package>
</struts>    
