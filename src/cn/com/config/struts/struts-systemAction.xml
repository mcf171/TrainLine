<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<package name="system2" extends="struts-default">

	<action name="systemaction_*" class="systemAction" method="{1}">
		<result name="cpu">
		/WEB-INF/jsp/backend/system/Myjsp.jsp
		</result>
		<result name="memory">
		/WEB-INF/jsp/backend/system/mymemory.jsp
		</result>
		<result name="disk">
		/WEB-INF/jsp/backend/system/mydisk.jsp
		</result>
		<result name="net">
		/WEB-INF/jsp/backend/system/mynetwork.jsp
		</result>
	</action>
	<action name="conductSystemaction" class="systemAction" method="getSystemPage">
		<result name="success">/WEB-INF/jsp/backend/system.jsp</result>
	</action>
</package>
</struts>    
 