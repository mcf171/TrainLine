<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<constant name = "struts.multipart.maxSize" value = "100000000" />

	<package name="course" namespace="/" extends="json-default">
		<action name="course_*" class="courseAction" method="{1}">
		<result type="json" name="success">
		<param name="root">dataMap</param>
		</result>
		<result name="detailCourseInfo">/WEB-INF/jsp/courseDetailInfo.jsp</result>
		<result name="intoStudyCenter">/WEB-INF/jsp/course.jsp</result>
		<result name="intoaddChapterPage">/WEB-INF/jsp/backend/study/addCourseChapter.jsp</result>
		<result type="json" name="json"></result>
		<result name="intoCoursePage">/WEB-INF/jsp/backend/study/class-system.jsp</result>
		<result name="addCourse">/WEB-INF/jsp/backend/study/addCourse.jsp</result>
		<param name="savePath">/upload</param>
		</action>
		
		<action name="getCourseStudyPage" class="courseAction"  method="getCourseStudyPage">
			<result name="two">/WEB-INF/jsp/study2.jsp</result>
			<result name="three">/WEB-INF/jsp/study.jsp</result>
		</action>
		
		<action name="getNormalDangJianKeCheng" class="courseAction"  method="getNormalDangJianKeCheng">
		<result type="json">
				<param name="root">dataMap</param>
				</result>
		</action>
		
		<action name="getNormalDangJianKeCheng" class="courseAction" method="getNormalDangJianKeCheng">
		<result type="json">
				<param name="root">dataMap</param>
				</result>
		</action>
	</package>
</struts>    
