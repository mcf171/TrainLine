<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<package name="record" extends="json-default">

	<action name="recordAction" class="recordAction" method="getAllRecord">
		<result  type="json">
				<param name="root">dataMap</param>
	    </result>
	</action>
	<action name="deleteRecordAction" class="recordAction" method="delete">
		<result name="delete" >
		  WEB-INF/jsp/backend/system/mySystemLog.jsp
		</result>
	</action>
	<action name="showPageAction" class="recordAction" method="execute">
		<result>WEB-INF/jsp/backend/system/mySystemLog.jsp </result>
	</action>
</package>
</struts>    
