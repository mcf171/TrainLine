<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	
	<package name="user" extends="system" >
	
   		<action name="login" class="userAction" method="login">
   			<result>/WEB-INF/jsp/index.jsp</result>
   			<result name="input">login.jsp</result>
   		</action>
   		
   		<action name="getBackendIndex" class="userAction" method="getBackendIndex">
   			<result>/WEB-INF/jsp/backend/index.jsp</result>
   		</action>
   		<action name="userInfo" class="userAction" method="show">
   			<result>/WEB-INF/jsp/userInfo.jsp</result>
   			<result name="input">index.jsp</result>
   		</action>
   	</package>
</struts>    
