<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="trainingClass" namespace="/" extends="json-default">
		<action name="trainingClass_*" class="trainingClassAction" method="{1}">
		<result type="json" name="success">
		<param name="root">dataMap</param>
		</result>
		<result type="json" name="json"></result>
		<result name="intoClasspage">/WEB-INF/jsp/backend/classes.jsp</result>
		<result name="classContent">/WEB-INF/jsp/backend/classes/classes.jsp</result>
		<result name="intoCertificatePage">/WEB-INF/jsp/backend/classes/certificateList.html</result>
		</action>
	</package>
</struts>    
